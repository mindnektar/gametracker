{
  "private": true,
  "scripts": {
    "dev": "knex migrate:latest && nodemon -L app.js --exec babel-node --ignore='node_modules' --presets @babel/preset-env --plugins @babel/plugin-proposal-object-rest-spread",
    "prestart": "knex migrate:latest",
    "start": "nodemon app.js --exec babel-node --ignore='node_modules' --presets @babel/preset-env --plugins @babel/plugin-proposal-object-rest-spread"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "nodemonConfig": {
    "verbose": true,
    "delay": "3000",
    "ext": "js,json,gql"
  },
  "dependencies": {
    "@babel/cli": "7.13.14",
    "@babel/core": "7.13.15",
    "@babel/node": "7.13.13",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/preset-env": "7.13.15",
    "@google/genai": "^0.10.0",
    "axios": "^0.21.1",
    "convict": "^6.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-jwt": "^6.0.0",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "7.0.4",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "merge-graphql-schemas": "^1.7.8",
    "moment": "^2.29.1",
    "nodemon": "^2.0.7",
    "objection": "^2.2.15",
    "pg": "^8.5.1",
    "redis": "^3.1.0",
    "serialize-error": "^8.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.2"
  },
  "resolutions": {
    "graphql": "15.5.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
