{
  "private": true,
  "scripts": {
    "dev": "knex migrate:latest && nodemon app.js --exec babel-node --ignore='node_modules' --presets @babel/preset-env --plugins @babel/plugin-proposal-object-rest-spread",
    "prestart": "knex migrate:latest",
    "start": "nodemon app.js --exec babel-node --ignore='node_modules' --presets @babel/preset-env --plugins @babel/plugin-proposal-object-rest-spread"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/node": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.12.1",
    "@babel/preset-env": "7.12.10",
    "convict": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-jwt": "^6.0.0",
    "graphql": "^15.4.0",
    "graphql-shield": "^7.4.3",
    "graphql-tools": "7.0.2",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.13",
    "merge-graphql-schemas": "^1.7.8",
    "moment": "^2.29.1",
    "nodemon": "^2.0.6",
    "objection": "^2.2.3",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "serialize-error": "^7.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5"
  },
  "resolutions": {
    "graphql": "15.4.0"
  }
}
